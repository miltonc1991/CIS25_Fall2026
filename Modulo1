#include <iostream> 
using namespace std;
int main() {
	//cout prints to the screen, the line below will print Hello Inventory System
	cout << "Hello Inventory System!\n";
	cout << "\tData Types:\n";
	cout << "Size of int: " << sizeof(int) << " bytes\n";
	cout << "Size of float: " << sizeof(float) << " bytes\n";
	cout << "Size of char: " << sizeof(char) << " bytes\n";
	return 0;
}
